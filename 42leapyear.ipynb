   "source": [
    "#Leap yr with in a range\n",
    "start=int(input(\"Enter starting yr:\"))\n",
    "end=int(input(\"Enter starting yr:\"))\n",
    "print (\"Leap year from\",start,\"to\",end,\"is:\")\n",
    "for year in range(start,end):\n",
    "    if(year%4==0 and year%100!=0 or year%400==0):\n",
    "        print(year)\n"
