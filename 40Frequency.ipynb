   "source": [
    "#frequency of characters\n",
    "s=input(\"Enter the string:\")\n",
    "f={}\n",
    "for i in s:\n",
    "    if i in f:\n",
    "        f[i]=f[i]+1\n",
    "    else:\n",
    "        f[i]=1\n",
    "for keys, value in f.items():\n",
    "    print(keys,\"is repeated\",value,\"time(s)\")"
   ]
